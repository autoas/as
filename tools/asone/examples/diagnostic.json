          "python": [
            "def CalculateKey(res):",
            "    seed = (res[2]<<24) + (res[3]<<16) + (res[4]<<8) +(res[5])",
            "    key = (seed^0x78934673)",
            "    return [(key>>24)&0xFF,(key>>16)&0xFF,(key>>8)&0xFF,key&0xFF]"
          ],
          "lua":[
            "function CalculateKey(res)",
            "    seed = (res[3]<<24) + (res[4]<<16) + (res[5]<<8) +(res[6])",
            "    key = seed ~ 0x78934673",
            "    return {(key>>24)&0xFF,(key>>16)&0xFF,(key>>8)&0xFF,key&0xFF}",
            "end"
          ]
        }
