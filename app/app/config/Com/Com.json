{
  "class": "Com",
  "E2E": "../E2E/E2E.json",
  "networks": [
    {
      "name": "CAN0",
      "network": "CAN",
      "device": "simulator_v2",
      "port": 0,
      "baudrate": 500000,
      "me": "AS",
      "groups": [
        {
          "SystemTime": ["year", "month", "day", "hour", "minute", "second"]
        }
      ],
      "dbc": "CAN0.dbc",
      "trigger": ["CanNmUserData"],
      "messages": [
        {
          "name": "SECOC_MSG0", "id": "0x99", "dlc": 36,
          "node": "AS", "CycleTime": 1000,
          "signals": [
            { "name": "VehicleConfig", "start": 7, "size": 288, "endian": "big", "InitialValue": "[i for i in range(32)]",
              "sign": "+", "factor": 1, "offset": 0, "min": 0, "max": 0, "node": [ "Other"] }
          ]
        },
        {
          "name": "SECOC_MSG1", "id": "0x98", "dlc": 36,
          "node": "Other",
          "signals": [
            { "name": "VehicleConfig2", "start": 7, "size": 288, "endian": "big",
              "sign": "+", "factor": 1, "offset": 0, "min": 0, "max": 0, "node": [ "AS"] }
          ]
        }
      ],
      "enable_message_tx_callout": true,
      "enable_message_rx_callout": true,
      "E2E": [ { "name": "RxMsgAbsInfo", "profile": "P11" },
               { "name": "TxMsgTime", "profile": "P11" } ]
    },
    {
      "name": "CAN1",
      "network": "CAN",
      "device": "simulator_v2",
      "port": 1,
      "baudrate": 500000,
      "me": "AS",
      "groups": [
        {
          "SystemTime_": ["year", "month", "day", "hour", "minute", "second"]
        }
      ],
      "dbc": "CAN0.dbc",
      "trigger": ["CanNmUserData"]
    }
  ]
}